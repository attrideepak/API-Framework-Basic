<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" verbose="10">
    <test name="groupbytest">
        <groups>
            <run>
                <include name="group-A"/>
                <include name="group-B"/>
            </run>
        </groups>
        <classes>
            <class name="test.ClassB">
                <methods>
                    <include name="methodA"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>